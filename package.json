    "@types/node": "^17.0.17",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "jest": "^27.5.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
