{
  "name": "banking_sparks_intern_web_app",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "private": true,
  "type": "commonjs",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "16.10.0"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "chance": "^1.1.8",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.0",
    "edge.js": "^5.3.2",
    "env-cmd": "^10.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.2",
    "express-edge": "^2.0.2",
    "express-rate-limit": "^6.2.1",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "nodemon --config nodemon.json src/server.ts",
    "build": " tsc --build --clean && tsc",
    "start": "node build/server.js",
    "db:migrate": "npm run typeorm migration:run",
    "db:revert": "npm run typeorm migration:revert",
    "db:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:build": "npm run db:revert && npm run db:migrate && npm run db:seed",
    "predeploy": " npm run db:build && npm run build"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
