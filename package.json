{
  "name": "banking_sparks_intern_web_app",
  "version": "1.0.0",
  "description": "",
  "main": "build/server.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "16.10.0"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "chance": "^1.1.8",
    "dotenv": "^16.0.0",
    "edge.js": "^5.3.2",
    "errorhandler": "^1.5.1",
    "express": "^4.17.2",
    "express-edge": "^2.0.2",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "typeorm-extension": "^1.1.0",
    "typeorm-seeding": "^1.6.1"
  },
  "scripts": {
    "test": "jest --runInBand --detectOpenHandles",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "nodemon --config nodemon.json src/server.ts",
    "build": "tsc --build --clean && tsc",
    "start": "node build/app.js",
    "db:dev:migrate": "npm run typeorm migration:run",
    "db:dev:revert": "npm run typeorm migration:revert",
    "db:dev:sync": "ts-node src/scripts/generateDb.ts",
    "db:dev:drop": "ts-node src/scripts/dropDb.ts",
    "db:dev:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "db:dev:build": "ts-node src/db/syncronizer.ts"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jest-environment-puppeteer": "^5.0.0",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.4",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0"
  }
}
